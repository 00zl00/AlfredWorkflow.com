				<string>require_once('workflows.php');
include('config.php');
$w = new Workflows();
$query = urlencode( "{query}" );

$url = "http://apps.wandoujia.com/api/v1/apps/?type=award&amp;opt_fields=title,icons.px48,installedCountStr,likesRate,packageName,latestApk.versionCode,award.*";
$suggestions = $w-&gt;request($url);
$suggestions = json_decode($suggestions);
foreach($suggestions as $suggest):
    $w-&gt;write($w-&gt;request($suggest-&gt;icons-&gt;px48),$suggest-&gt;packageName.'.png','appicons');
    $w-&gt;result( $suggest-&gt;packageName, 
                '--cookie "'.$cookie.'" --data "did='.$did.'&amp;title='.urlencode(strip_tags($suggest-&gt;title)).'&amp;url=http://apps.wandoujia.com/apps/'.$suggest-&gt;packageName.'/download&amp;versioncode='.$suggest-&gt;latestApk-&gt;versionCode.'&amp;packagename='.$suggest-&gt;packageName.'" http://www.wandoujia.com/offlinepush/push',
