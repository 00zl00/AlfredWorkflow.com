				<key>script</key>
				<string>#!/bin/bash

if [ -f "ip" ]; then
    if [ -f "oldgw" ]; then
        OLDGW=`cat oldgw`;
        for ip in ` awk '{print $1}' ip`
        {
            echo "{query}" | sudo -S route delete $ip $OLDGW &gt;/dev/null;
        }
        rm -rf oldgw;

		long=`sw_vers -productVersion`
		short=${long:0:4}
		if [ "$short" == "10.4" ]; then
		    lookupd -flushcache
		elif [ "$short" == "10.5" ] || [ "$short" == "10.6" ]; then
		    dscacheutil -flushcache
		elif [ "$short" == "10.7" ] || [ "$short" == "10.8" ]; then
		    sudo killall -HUP mDNSResponder
		fi

        echo "路由表清除完毕";
    else
        echo "路由表未被添加";
    fi
else
    echo "数据文件不存在，请先更新IP数据";
fi</string>
