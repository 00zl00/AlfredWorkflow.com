				<key>script</key>
				<string>#!/bin/bash

if [ -f "ip" ]; then
    if [ -f "oldgw" ]; then
        echo "路由表已经存在，请先运行路由表清除命令";
    else
        OLDGW=`netstat -nr | grep '^default' | grep -v 'ppp' | sed 's/default *\([0-9\.]*\) .*/\1/' | sed '/^$/d'`
        for ip in ` awk '{print $1}' ip`
        {
            echo "{query}" | sudo -S route add $ip $OLDGW &gt;/dev/null;
        }
        echo $OLDGW &gt; oldgw;

		long=`sw_vers -productVersion`
		short=${long:0:4}
		if [ "$short" == "10.4" ]; then
		    lookupd -flushcache
		elif [ "$short" == "10.5" ] || [ "$short" == "10.6" ]; then
		    dscacheutil -flushcache
		elif [ "$short" == "10.7" ] || [ "$short" == "10.8" ]; then
		    sudo killall -HUP mDNSResponder
		fi

        echo "路由表添加完毕";
    fi
else
    echo "数据文件不存在，请先更新IP数据";
fi</string>
